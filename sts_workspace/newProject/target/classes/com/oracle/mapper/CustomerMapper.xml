<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.oracle.dao.CustomerDao">
<insert id="addCustomer" parameterType="customer" useGeneratedKeys="true" keyProperty="customer_Id">
	insert into tcustomer values(null,#{id_Type},#{id_Number},#{customer_Name},#{customer_Birthday},#{customer_Sex},#{customer_Address})
</insert>
</mapper>